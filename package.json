{
  "name": "lw-data",
  "version": "0.3.0",
  "description": "Laurel & Wolf Cowboy Data",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint && hihat __test__/index.js __test__/sdk/*.js __test__/serialize/*.js -- --transform [babelify --stage 0] -p tap-dev-tool",
    "test-ci": "zuul -- __test__/index.js __test__/sdk/*.js __test__/serialize/*.js",
    "lint": "lw lint src/ --cache .tmp/lint.json",
    "clean": "rm -rf ./dist",
    "bundle": "babel --stage 0 src --out-dir dist",
    "postinstall": "npm run clean && npm run bundle"
  },
  "dependencies": {
    "as-array": "^2.0.0",
    "is-number": "^2.0.2",
    "is-promise": "^2.0.0",
    "join-path": "^1.1.0",
    "lodash": "^3.10.0",
    "pluralize": "^1.1.5"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.2.0",
    "es6-promise": "^3.0.2",
    "lw-build": "^1.1.2",
    "mock-fetch": "^1.0.0",
    "tap-dev-tool": "^1.3.0",
    "tap-spec": "^4.1.0",
    "tessed": "^2.0.0",
    "zuul": "^3.3.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": "0"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurelandwolf/data.git"
  },
  "author": "Laurel & Wolf",
  "bugs": {
    "url": "https://github.com/laurelandwolf/data/issues"
  },
  "homepage": "https://github.com/laurelandwolf/data#readme",
  "license": "MIT"
}
