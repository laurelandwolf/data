{
  "name": "lw-data",
  "version": "0.0.9",
  "description": "Data",
  "main": "src/index.js",
  "scripts": {
    "test-hihat": "hihat __test__/state/*.js __test__/sdk/*.js __test__/serialize/*.js -- -t [babelify --stage 0] -p tap-dev-tool",
    "test": "npm run lint && browserify __test__/sdk/*.js __test__/serialize/*.js | tap-closer | smokestack | tap-spec",
    "test-watch": "onchange __test__/*.js __test__/**/*.js src/**/*.js src/*.js -- npm test",
    "test-ci": "zuul -- __test__/state/*.js __test__/sdk/*.js __test__/serialize/*.js",
    "lint": "lw lint src/ --cache .tmp/lint.json",
    "bundle": "NODE_ENV=production browserify -t [babelify --stage 0] src/index.js > bundle.js"
  },
  "dependencies": {
    "as-array": "^1.0.0",
    "immutable": "^3.7.4",
    "is-number": "^2.0.2",
    "is-promise": "^2.0.0",
    "join-path": "^1.0.1",
    "lodash": "^3.10.0",
    "pluralize": "^1.1.2",
    "raf": "^3.1.0",
    "reselect": "0.0.2"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browserify": "^11.0.1",
    "es6-promise": "^2.3.0",
    "hihat": "^2.4.1",
    "lw-build": "github:laurelandwolf/build",
    "minifyify": "^7.0.3",
    "mock-fetch": "^1.0.0",
    "onchange": "^2.0.0",
    "react": "^0.13.3",
    "smokestack": "^3.3.0",
    "tap-closer": "^1.0.0",
    "tap-dev-tool": "^1.3.0",
    "tap-spec": "^4.0.2",
    "tessed": "^2.0.0",
    "zuul": "^3.2.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": "0"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurelandwolf/data.git"
  },
  "author": "Laurel & Wolf",
  "bugs": {
    "url": "https://github.com/laurelandwolf/data/issues"
  },
  "homepage": "https://github.com/laurelandwolf/data#readme"
}
